
Proceso Ejercicio_1
	
	
	Definir num1,num2 como Real;
	
	Escribir Sin Saltar "Escribe el nº 1: ";
	
	Leer num1;
	
	Escribir Sin Saltar "Escribe el nº 2: ";
	
	Leer num2;
	
	Si num1 > num2 Entonces
		
		Escribir "El número ", num1, " es mayor que ", num2, ".";
		
	SiNo
		
		Escribir "Fin del programa.";
		
	FinSi
	
	
	
FinProceso
