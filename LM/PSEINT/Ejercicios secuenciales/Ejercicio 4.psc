
Proceso Ejercicio_4
	
	Definir num1,num2,suma,resta,div,mult como Real;
	
	Escribir Sin Saltar "Número a: ";
	
	Leer num1;
	
	Escribir Sin Saltar "Número b: ";
	
	Leer num2;
	
	suma <- num1 + num2;
	
	resta <- num1 - num2;
	
	div <- num1 / num2;
	
	mult <- num1 * num2;
	
	Escribir "La Suma sería: ", suma, ".";
	
	Escribir "La Resta sería: ", resta, ".";
	
	Escribir "La División sería: ", div, ".";
	
	Escribir "La Multiplicación sería: ", mult, ".";
	
FinProceso
