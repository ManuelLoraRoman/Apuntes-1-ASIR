
Proceso Ejercicio_6
	
	Definir num1,num2,num3,media Como Real;
	
	Escribir Sin Saltar "Número 1: ";
	
	Leer num1;
	
	Escribir Sin Saltar "Número 2: ";
	
	Leer num2;
	
	Escribir Sin Saltar "Número 3: ";
	
	Leer num3;
	
	media <- (num1 + num2 + num3) / 3;
	
	Escribir "La media de los tres números sería: ", media, ".";
	
FinProceso
