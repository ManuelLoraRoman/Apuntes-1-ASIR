
Proceso Ejercicio_10
	
	Definir n,m como Real;
	
	n <- 1;
	
	m <- 0;
	
	Mientras n < 6 Hacer
		
		Escribir "TABLA DE MULTIPLICAR DEL ", n , ":";
		
		Escribir "-------------------------------";
		
		Mientras m < 11 Hacer
			
			Escribir m ," x ", n , " = " , m * n;
			
			m <- m + 1;
			
		FinMientras
		
		m <- 0;
		
		n <- n + 1;
		
	FinMientras
	
FinProceso
