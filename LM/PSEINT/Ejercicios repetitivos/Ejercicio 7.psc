
Proceso Ejercicio_7
	
	Definir numtabla,n como Real;
	
	Escribir Sin Saltar "Escriba un número: ";
	
	Leer numtabla;
	
	n <- 0;
	
	Escribir "TABLA DE MULTIPLICAR DEL ", numtabla , ":";
	
	Escribir "-------------------------------";
	
	Mientras n < 11 Hacer
		
		Escribir n ," x ", numtabla , " = " , n * numtabla;
		
		n <- n + 1;
		
	FinMientras
	
FinProceso
